{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/webhook",
            "dest": "/api/webhook"
        },
        {
            "src": "/health", 
            "dest": "/api/health"
        },
        {
            "src": "/",
            "dest": "/api/index"
        }
    ]
}
